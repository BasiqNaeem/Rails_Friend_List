<div id="<%= dom_id friend %>">


<div class="jumbotron">
  <h3 class="display-4"><%= friend.first_name %> <%= friend.last_name %></h3>
  <p class="lead"> <%= friend.email %> | <%= friend.phone %> | <%= friend.twitter %> </p>
  <hr class="my-4">
   <br>
  
  <%= link_to "Edit ", edit_friend_path(@friend), class:"btn btn-secondary" %>  |
   <%= link_to "Back ", friends_path,class:"btn btn-dark my-3 " %>
</div>
